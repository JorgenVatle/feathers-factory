{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2022",
    "moduleResolution": "bundler",
    "declaration": true,
    "noEmit": true,
    "outDir": "./dist",

    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true

  },
}
